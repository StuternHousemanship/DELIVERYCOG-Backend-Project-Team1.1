{
    "name": "deliverycog-backend-project-team1.1",
    "version": "1.0.0",
    "description": "An interstate delivcery platform for people to send items to other geographical states through individuals travelling to those locations.",
    "main": "app.js",
    "scripts": {
        "start": "node dist/app.ts",
        "lint": "eslint --ext .ts . --fix",
        "prettier": "prettier --config .prettierrc --ignore-path .gitignore  --write './src/**/*.+(js|ts)'",
        "format": "npm run prettier ",
        "migrate:dev:up": "npx knex migrate:latest --knexfile ./src/V1/APIs/Config/knexfile.ts --env development ",
        "migrate:dev:down": "npx knex migrate:down  --knexfile ./src/V1/APIs/Config/knexfile.ts --env development ",
        "migrate:prod:up": "npx knex migrate:latest --knexfile ./src/V1/APIs/Config/knexfile.ts --env production ",
        "migrate:prod:down": "npx knex migrate:down  --knexfile ./src/V1/APIs/Config/knexfile.ts --env production ",
        "dev": "npm run build && nodemon ./src/app.ts",
        "build": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/StuternHousemanship/DELIVERYCOG-Backend-Project-Team1.1.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/StuternHousemanship/DELIVERYCOG-Backend-Project-Team1.1/issues"
    },
    "homepage": "https://github.com/StuternHousemanship/DELIVERYCOG-Backend-Project-Team1.1#readme",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/html-to-text": "^8.1.1",
        "@types/jasmine": "^3.10.3",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/node": "^18.8.5",
        "@types/nodemailer": "^6.4.6",
        "@types/pg": "^8.6.5",
        "@types/supertest": "^2.0.11",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "eslint": "^8.25.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jasmine": "^4.3.0",
        "jasmine-spec-reporter": "^7.0.0",
        "nodemon": "^2.0.16",
        "prettier": "^2.7.1",
        "supertest": "^6.2.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-validator": "^6.14.2",
        "html-to-text": "^8.2.1",
        "jsonwebtoken": "^8.5.1",
        "knex": "^2.3.0",
        "nodemailer": "^6.7.8",
        "objection": "^3.0.1",
        "pg": "^8.8.0",
        "swagger-jsdoc": "^6.2.5",
        "swagger-ui-express": "^4.5.0"
    }
}
